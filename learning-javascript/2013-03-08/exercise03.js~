//Esercizio3a,3b
function upperword(s){
return s.toUpperCase();
}

//Esercizio3c
var data = [
  {id:'01', name:'duffy'},
  {id:'02', name:'michey'},
  {id:'03', name:'donald'},
  {id:'04', name:'goofy'},
  {id:'05', name:'minnie'},
  {id:'06', name:'scrooge'}
];
var key = 'name';
var values = ['goofy', 'scrooge'];


var filterResult = data.filter(function(item, index, array){
	return (item.name == values)});

filterResult;

